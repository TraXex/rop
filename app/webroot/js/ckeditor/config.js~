CKEDITOR.editorConfig = function( config )
{
	config.filebrowserBrowseUrl = 'Cakeck/app/webroot/upload/type=files';
   config.filebrowserImageBrowseUrl = 'Cakeck/app/webroot/upload/?type=images';
   config.filebrowserFlashBrowseUrl = 'Cakeck/app/webroot/upload/?type=flash';
   config.filebrowserUploadUrl = 'Cakeck/app/webroot/upload/?type=files';
   config.filebrowserImageUploadUrl = 'Cakeck/app/webroot/upload/?type=images';
   config.filebrowserFlashUploadUrl = 'Cakeck/app/webroot/upload/?type=flash';

};
	   